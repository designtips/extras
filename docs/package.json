{
  "name": "boilerplate-webpack-babel",
  "version": "0.0.1",
  "author": "Dmitry Sheiko (http://dsheiko.com)",
  "keywords": [
    "webpack",
    "babel",
    "optimization",
    "starter",
    "boilerplate",
    "hot"
  ],
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js && webpack --config webpack.prod-legacy.js",
    "watch": "webpack --watch  --config webpack.dev.js",
    "start": "http-server . -o -c-1",
    "lint:js": "jscs src",
    "lint:js:fix": "jscs src -x"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "http-server": "^0.10.0",
    "jscs": "^3.0.7",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "license": "ISC",
  "dependencies": {
    "console-log-html": "^2.0.2"
  }
}
