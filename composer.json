{
  "name": "dsheiko/extras",
  "type": "library",
  "description": "Collection of chainable high-order functions to abstract and manipulate PHP types",
  "keywords": [
    "underscore",
    "lodash",
    "utilities",
    "library",
    "abstract"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Sheiko",
      "homepage": "http://dsheiko.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "peridot-php/leo": "^1.6",
    "peridot-php/peridot": "^1.19",
    "peridot-php/peridot-code-coverage-reporters": "^3.0",
    "squizlabs/php_codesniffer": "^3.1",
    "apigen/apigen": "dev-master",
    "roave/better-reflection": "dev-master#c87d856"
  },
  "autoload": {
    "psr-4": {
      "Dsheiko\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "test": "vendor/bin/peridot tests/",
    "coverage": "./vendor/bin/peridot  tests/ -c peridot.config.php --reporter html-code-coverage"
  },
  "config": {
    "sort-packages": true
  }
}
